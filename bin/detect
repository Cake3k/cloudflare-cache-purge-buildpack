#!/bin/sh

# Export the environment variables from their on-disk location
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
./${DIR}/bin/export_env_dir

if [ -z ${CF_AUTH_KEY+x} ]; then
  echo "       No purgeable cache credentials"
else
  echo "       Detected purgeable Cloudflare cache credentials"
fi
